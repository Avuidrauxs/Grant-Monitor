    <servlet>
        <servlet-name>SessionServlet</servlet-name>
        <servlet-class>Servlets.SessionServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LogoutServlet</servlet-name>
        <servlet-class>Servlets.LogoutServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>InputsServlet</servlet-name>
        <servlet-class>Servlets.InputsServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AlternateSession</servlet-name>
        <servlet-class>Servlets.AlternateSession</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AlternateLogout</servlet-name>
        <servlet-class>Servlets.AlternateLogout</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ReportsServlet</servlet-name>
        <servlet-class>Servlets.ReportsServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>RPInputs</servlet-name>
        <servlet-class>Servlets.RPInputs</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ITeditServlet</servlet-name>
        <servlet-class>Servlets.ITeditServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>SessionServlet</servlet-name>
        <url-pattern>/SessionServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LogoutServlet</servlet-name>
        <url-pattern>/LogoutServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>InputsServlet</servlet-name>
        <url-pattern>/InputsServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AlternateLogout</servlet-name>
        <url-pattern>/AlternateLogout</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AlternateSession</servlet-name>
        <url-pattern>/AlternateSession</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ReportsServlet</servlet-name>
        <url-pattern>/ReportsServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>RPInputs</servlet-name>
        <url-pattern>/RPInputs</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ITeditServlet</servlet-name>
        <url-pattern>/ITeditServlet</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
